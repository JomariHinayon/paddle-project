{
  "name": "paddle-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "node fix-netlify-build.js && NEXT_IGNORE_ESLINT=1 next build",
    "build:static": "node fix-netlify-build.js && NEXT_IGNORE_ESLINT=1 NEXT_USE_STATIC_EXPORT=true next build",
    "netlify-build": "npm run netlify-install && node fix-netlify-build.js && NEXT_IGNORE_ESLINT=1 SKIP_TYPE_CHECK=1 next build",
    "netlify-install": "npm install",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@types/firebase": "^2.4.32",
    "autoprefixer": "^10.4.21",
    "axios": "^1.8.4",
    "dotenv": "^16.5.0",
    "firebase": "^11.7.1",
    "firebase-admin": "^13.2.0",
    "micro": "^10.0.1",
    "next": "15.2.4",
    "postcss": "^8.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.4.1",
    "tailwindcss": "^3.3.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "typescript": "^5"
  }
}
